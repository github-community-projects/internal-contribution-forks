    "@types/ws": "8.5.10",
    "abort-controller": "3.0.0",
    "concurrently": "8.2.2",
    "dotenv-load": "3.0.0",
    "eslint": "8.31.0",
    "eslint-config-next": "13.2.3",
    "jest": "29.7.0",
    "nock": "13.3.0",
    "smee-client": "1.2.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
