    "@octokit/rest": "20.0.2",
    "@octokit/types": "12.4.0",
    "@primer/octicons-react": "19.8.0",
    "@primer/react": "36.3.1",
    "@tanstack/react-query": "4.36.1",
    "@trpc/client": "10.44.1",
    "@trpc/next": "10.44.1",
    "@trpc/react-query": "10.44.1",
    "@trpc/server": "10.44.1",
    "next": "13.5.5",
    "next-auth": "4.24.5",
    "probot": "12.3.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "simple-git": "3.21.0",
    "styled-components": "5",
    "swr": "2.2.4",
    "tempy": "1.0.1",
    "tslog": "4.9.2",
    "zod": "3.22.4"
